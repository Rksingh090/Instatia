<script>
    $(document).ready(function () {
        $('#showcs').hide();
        $('#showvd ').hide();
        $('#home').on('click', function () {
            $('#showcs').hide();
            $('#showvd').hide();
            $('#showhome').show();

            $('.t-btn').removeClass('btn-primary').addClass('btn-secondary');
            $(this).removeClass('btn-secondary').addClass('btn-primary')
        });
        $('#cs').on('click', function () {
            $('#showhome').hide();
            $('#showvd').hide();
            $('#showcs').show();

            $('.t-btn').removeClass('btn-primary').addClass('btn-secondary');
            $(this).removeClass('btn-secondary').addClass('btn-primary')
        });
        $('#vd').on('click', function () {
            $('#showhome').hide();
            $('#showvd').show();
            $('#showcs').hide();

            $('.t-btn').removeClass('btn-primary').addClass('btn-secondary');
            $(this).removeClass('btn-secondary').addClass('btn-primary')
        });
    });
</script>


    
    
    
    
